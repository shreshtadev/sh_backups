        {
            "name": "Run Sh Backups - Upload",
            "type": "go",
            "request": "launch",
            "args": [
                "-U"
            ],
            "mode": "auto",
            "program": "${cwd}"
        },
        {
            "name": "Run Sh Backups - Delete",
            "type": "go",
            "request": "launch",
            "args": [
                "-D"
            ],
            "mode": "auto",
            "program": "${cwd}"
        },
        {
            "name": "Run Sh Backups - ForeDelete",
            "type": "go",
            "request": "launch",
            "args": [
                "-FD"
            ],
            "mode": "auto",
            "program": "${cwd}"
        }
    ]
